package com.dinkydetails.telly;

import model.Contact;
import android.app.Activity;
import android.content.Intent;
import android.os.Bundle;
import android.view.Window;

/** New Activity that will run when the app is launched.  */
/** Need to move the Main Activity to a different location... but still have it prompt, maybe...  */
public class AddContactActivity  extends Activity{

	
	@Override
	protected void onCreate(Bundle savedInstanceState) 
	{
		super.onCreate(savedInstanceState);
		requestWindowFeature(Window.FEATURE_NO_TITLE);
		setContentView(R.layout.add_contact);
		
		init();
		
		Intent iContactListActivity = getIntent();
		
		cObj = (Contact) iContactListActivity.getSerializableExtra(AppConstants.CURRENT_OBJ);
		
		if(cObj != null)
		setInformation(cObj);<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	    xmlns:tools="http://schemas.android.com/tools"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:background="@color/black"
	    >
			
		
	}
	
}
